! Fonts {{{
Xft.autohint: false
Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.hintstyle: hintslight
Xft.dpi:       96
! }}}

! rxvt-unicode {{{
URxvt*font:                 xft:Noto Sans Mono:regular:size=12
URxvt*imFont:               xft:Noto Sans Mono:italic:size=12
URxvt*boldFont:             xft:Noto Sans Mono:bold:size=12
URxvt*italicFont:           xft:Noto Sans Mono:italic:size=12
URxvt*boldItalicFont:       xft:Noto Sans Mono:bold italic:size=12
URxvt.depth:    24
URxvt.geometry: 92x24
URxvt.loginShell: true
URxvt.lineSpace: 0
URxvt.internalBorder: 10
URxvt*scrollBar: false
URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.url-select.launcher: /sbin/xdg-open
URxvt.url-select.underline: true
URxvt.clipboard.autocopy: true
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.M-u: perl:url-select:select_next

! }}}
